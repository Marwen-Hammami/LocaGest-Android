<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listfichevoitureRowModel" type="com.chihebsapplication.app.modules.fichesreparation.data.model.ListfichevoitureRowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackfichevoiture"
        android:layout_height="wrap_content" android:layout_width="match_parent">
        <LinearLayout android:id="@+id/linearColumnfichevoiture"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_1">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtFichevoiture"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_114pxv"
                android:layout_marginEnd="@dimen/_18pxh"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{listfichevoitureRowModel.txtFichevoiture}"
                style="@style/txtPoppinsregular15" tools:text="@string/msg_fiche_voiture"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearUser1"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_40pxv"
            android:layout_marginStart="@dimen/_9pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageOval"
                android:layout_height="@dimen/_52pxh"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_width="@dimen/_49pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_oval}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_oval"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnjohnnyrios"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_weight="0.85" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJohnnyRios"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{listfichevoitureRowModel.txtJohnnyRios}"
                    style="@style/txtAkayaKanadakaregular15" tools:text="@string/msg_chiheb_ben_abde"/>
                <View android:background="@color/gray_200"
                    android:id="@+id/lineLine"
                    android:layout_height="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_30pxv" android:layout_width="@dimen/_272pxh"/>
            </LinearLayout>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtHeading"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_32pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{listfichevoitureRowModel.txtHeading}"
            style="@style/txtPoppinslight13" tools:text="@string/msg_1_nov_2023_16"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtHeadingOne"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_13pxv"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{listfichevoitureRowModel.txtHeadingOne}"
            style="@style/txtPoppinsmedium13" tools:text="@string/lbl_en_cours"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnLivrerVoiture"
            android:layout_gravity="bottom|end"
            android:layout_height="@dimen/_31pxv"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="@dimen/_124pxh"
            android:letterSpacing="0.02"
            android:paddingBottom="@dimen/_5pxv"
            android:paddingEnd="@dimen/_15pxh"
            android:paddingStart="@dimen/_13pxh"
            android:paddingTop="@dimen/_5pxv" android:singleLine="true"
            android:text="@string/lbl_livrer_voiture"
            android:textAllCaps="false" style="@style/btnSolidRounded" tools:text="@string/lbl_livrer_voiture"/>
    </FrameLayout>
</layout>
